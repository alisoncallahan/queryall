<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.queryall</groupId>
	<artifactId>queryall-lib</artifactId>
	<packaging>jar</packaging>
	<version>1.0.2-SNAPSHOT</version>
	<name>queryall</name>
	<url>http://bio2rdf.wiki.sourceforge.net</url>
	<inceptionYear>2007</inceptionYear>
	<developers>
		<developer>
			<id>p_ansell</id>
			<email>p_ansell@yahoo.com</email>
			<name>Peter Ansell</name>
		</developer>
	</developers>
	<licenses>
		<license>
			<name>GNU General Public License 3.0</name>
			<url>http://www.gnu.org/licenses/gpl-3.0.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<mailingLists>
		<mailingList>
			<name>Bio2RDF Google Group</name>
			<archive>https://groups.google.com/group/bio2rdf/</archive>
			<post>bio2rdf@googlegroups.com</post>
			<subscribe>https://groups.google.com/group/bio2rdf/subscribe</subscribe>
			<unsubscribe>https://groups.google.com/group/bio2rdf/subscribe</unsubscribe>
		</mailingList>
	</mailingLists>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<scm>
		<connection>scm:git:git://bio2rdf.git.sourceforge.net/gitroot/queryall/queryall</connection>
		<developerConnection>scm:git:ssh://p_ansell@bio2rdf.git.sourceforge.net/gitroot/queryall/queryall</developerConnection>
		<url>http://bio2rdf.git.sourceforge.net/</url>
	</scm>
	<dependencies>
		<dependency>
			<groupId>org.queryall</groupId>
			<artifactId>queryall-api</artifactId>
			<version>1.0.2-SNAPSHOT</version>
			<scope>compile</scope>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>org.queryall</groupId>
			<artifactId>queryall-api</artifactId>
			<version>1.0.2-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.2</version>
			<scope>test</scope>
			<type>jar</type>
		</dependency>
		<dependency>
			<artifactId>log4j</artifactId>
			<groupId>log4j</groupId>
			<version>1.2.16</version>
			<scope>compile</scope>
			<type>jar</type>
		</dependency>
		<dependency>
			<artifactId>sesame-model</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-repository-sail</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-rio-ntriples</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-rio-rdfxml</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-rio-n3</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-rio-trix</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-rio-trig</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>rdfjson</artifactId>
			<groupId>net.fortytwo.sesametools</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>1.3</version>
		</dependency>
		<dependency>
			<artifactId>nquads</artifactId>
			<groupId>net.fortytwo.sesametools</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>1.3</version>
		</dependency>
		<dependency>
			<artifactId>sesame-sail-memory</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<artifactId>sesame-queryparser-sparql</artifactId>
			<groupId>org.openrdf.sesame</groupId>
			<type>jar</type>
			<scope>compile</scope>
			<version>2.5.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-log4j12</artifactId>
			<version>1.6.1</version>
			<type>jar</type>
			<scope>runtime</scope>
		</dependency>
	</dependencies>
	<build>
		<resources>
			<resource>
				<directory>src/test/resources</directory>
				<filtering>true</filtering>
			</resource>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
			<!-- Added test excludes for Abstract -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.9</version>
				<configuration>
					<excludes>
						<exclude>**/Abstract*</exclude>
					</excludes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.3.1</version>
				<executions>
					<execution>
						<goals>
							<goal>test-jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>2.1.2</version>
				<executions>
					<execution>
						<id>attach-source</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>2.8</version>
				<executions>
					<execution>
						<id>attach-javadoc</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>aduna-repo</id>
			<name>Aduna repository</name>
			<url>http://repo.aduna-software.org/maven2/releases</url>
			<releases><enabled>true</enabled></releases>
		</repository>
		<repository>
			<id>aduna-snapshot-repo</id>
			<name>Aduna snapshot repository</name>
			<url>http://repo.aduna-software.org/maven2/snapshots</url>
			<snapshots><enabled>true</enabled></snapshots>
		</repository>
		<repository>
			<id>on.cs.unibas.ch</id>
			<name>DBIS Maven Releases Repository</name>
			<url>http://on.cs.unibas.ch/maven/repository</url>
			<releases><enabled>true</enabled></releases>
			<snapshots><enabled>false</enabled></snapshots>
		</repository>
		<repository>
			<id>fortytwo.net</id>
			<name>SesameTools Repository</name>
			<url>http://fortytwo.net/maven2</url>
			<releases><enabled>true</enabled></releases>
			<snapshots><enabled>false</enabled></snapshots>
		</repository>
	</repositories>
</project>
